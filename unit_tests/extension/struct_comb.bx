type al1 = int;
type al2 = bool; 
type sss = struct {first : int, second : sss} ;

def main(){
    var a = 1 : al1;
    var b = true : al2;
    var c = 0 : sss;

    c.first = 5;
    c.second = 7;

    var p = &c : sss*;
    

    print(a);
    print(b);

    print(c.second);

    print(p->first);
    print(p->second);
}